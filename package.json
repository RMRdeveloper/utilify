{
  "name": "utilifycore",
  "version": "1.10.2",
  "main": "dist/index.cjs",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs",
      "types": "./dist/index.d.ts"
    }
  },
  "scripts": {
    "build": "rimraf dist && rollup -c",
    "prepare": "husky",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage"
  },
  "keywords": [
    "utility",
    "library",
    "javascript",
    "typescript",
    "utils",
    "helpers",
    "tools"
  ],
  "author": "Ronald Moreno <rmorenorodriguez16@gmail.com>",
  "license": "MIT",
  "description": "A utility library for JavaScript and TypeScript projects.",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/RMRdeveloper/utilify.git"
  },
  "bugs": {
    "url": "https://github.com/RMRdeveloper/utilify/issues"
  },
  "homepage": "https://github.com/RMRdeveloper/utilify#readme",
  "files": [
    "dist/**/*",
    "!dist/tests/**/*"
  ],
  "devDependencies": {
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.3.0",
    "@types/jest": "^30.0.0",
    "@types/node": "^24.9.1",
    "husky": "^9.1.7",
    "jest": "^30.2.0",
    "prettier": "^3.6.2",
    "rimraf": "^5.0.10",
    "rollup": "^4.52.5",
    "rollup-plugin-dts": "^6.2.3",
    "ts-jest": "^29.4.5",
    "tslib": "^2.8.1",
    "typescript": "^5.9.3"
  }
}
